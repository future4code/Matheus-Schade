```function calculaNota(ex, p1, p2) {
let mediaFinal = ((ex * 1) + (p1 * 2) + (p2 * 3)) / 6

    if (mediaFinal >= 9) {
        return bobagem = "A"
    } else if (mediaFinal >= 7.5 && mediaFinal < 9) {
        return bobagem = "B"
    } else if (mediaFinal >= 6 && mediaFinal < 7.5) {
        return bobagem = "C"
    } else {
        return bobagem = "D"
    }
}```